{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "./dist/electron/main.js",
  "scripts": {
    "start": "npm run dev",
    "build": "node .electron-react/build.js",
    "build:darwin": "cross-env BUILD_TARGET=darwin node .electron-react/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node .electron-react/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node .electron-react/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node .electron-react/build.js",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-react/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-react/build.js",
    "dev": "node .electron-react/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-react/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-react/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run lint:fix"
  },
  "repository": "https://github.com/gofugui/electron-react",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "babel-eslint": "^8.2.3",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.4.5",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "del": "^5.1.0",
    "electron": "^6.0.2",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-log": "^3.0.7",
    "electron-packager": "^12.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.14.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "react": "^16.6.0",
    "react-devtools": "^3.6.3",
    "react-dom": "^16.6.0",
    "sass-loader": "^7.3.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "fs-extra": "^8.1.0",
    "git": "^0.1.5",
    "lowdb": "^1.0.0",
    "shelljs": "^0.8.3",
    "uuid": "^3.3.3"
  }
}
